{
  "name": "Cerveau",
  "version": "1.0.0",
  "description": "The Node.js implementation of a multi-game server intended for games with AIs within the Cadre framework.",
  "repository": {
    "type": "git",
    "url": "git://github.com/siggame/Cerveau"
  },
  "private": true,
  "dependencies": {
    "argparse": "1.0.6",
    "cadre-js-extensions": "siggame/Cadre-js-extensions",
    "chess.js": "0.9.3",
    "classe": "1.0.0",
    "clone": "1.0.2",
    "colors": "1.1.2",
    "express": "4.13.4",
    "express-handlebars": "3.0.0",
    "extend": "3.0.0",
    "gaussian": "1.1.0",
    "git-rev": "0.2.1",
    "hosted-git-info": "2.4.2",
    "lark-websocket": "JacobFischer/lark-websocket",
    "mathjs": "3.18.0",
    "moment": "2.11.2",
    "request": "2.81.0",
    "sanitize-filename": "1.5.3",
    "seedrandom": "2.4.2",
    "simple-git": "1.70.0",
    "uuid": "2.0.1"
  },
  "devDependencies": {
    "apidoc": "0.16.1",
    "eslint": "3.7.1",
    "ghooks": "1.3.2"
  },
  "config": {
    "ghooks": {
      "pre-commit": "eslint --cache ./",
      "post-merge": "npm install",
      "post-rewrite": "npm install"
    }
  },
  "engines": {
    "node": "6.6.0"
  },
  "scripts": {
    "lint": "eslint --cache ./",
    "apidoc": "apidoc -i ./website/ -o ./api-docs",
    "fix": "eslint --cache ./ --fix"
  },
  "license": "MIT"
}
